{
  "name": "huskygpt",
  "version": "0.1.6",
  "description": "Automatically generate unit tests for your code using openai gpt-4",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "rimraf lib && tsc ../test/index.ts --outDir lib --esModuleInterop",
    "build-watch": "rimraf lib && tsc ../test/index.ts --outDir lib --esModuleInterop --watch",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "huskygpt": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luffy-xu/huskygpt"
  },
  "keywords": [
    "auto",
    "generate",
    "test",
    "gpt4",
    "openai"
  ],
  "author": "https://github.com/luffy-xu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luffy-xu/huskygpt/issues"
  },
  "homepage": "https://github.com/luffy-xu/huskygpt#readme",
  "dependencies": {
    "@babel/generator": "^7.21.3",
    "@babel/parser": "^7.21.3",
    "@babel/traverse": "^7.21.3",
    "commander": "^10.0.0",
    "dotenv": "^16.0.3",
    "openai": "^3.2.1",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "rimraf": "^4.4.1",
    "typescript": "^5.0.2"
  }
}
